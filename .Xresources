! Xft settings

Xft.dpi:                    96
Xft.antialias:              true
Xft.rgba:                   rgb
Xft.hinting:                true
Xft.hintstyle:              hintslight

! ------------------------------------------------------------------------------
! ROFI Color theme
! ------------------------------------------------------------------------------
rofi.color-enabled: true
rofi.color-window: argb:d9151516, #151516, #151516
rofi.color-normal: #151516, #27272d, #151516, #151516, #f8f2f4
rofi.color-active: #8be6ea, #27272d, #151516, #151516, #f8f2f4
rofi.color-urgent: #151516, #27272d, #151516, #151516, #f8f2f4

! special
*.foreground:   #f8f2f4
*.background:   #151516
*.cursorColor:  #f8f2f4

! black
*.color0:       #27272d
*.color8:       #3a3a3d

! red
*.color1:       #d4617e
*.color9:       #c47488

! green
*.color2:       #73da8e
*.color10:      #89e4a1

! yellow
*.color3:       #ced686
*.color11:      #e0e5b6

! blue
*.color4:       #38bdc3
*.color12:      #8be6ea

! magenta
*.color5:       #e39ab8
*.color13:      #f6c6ff

! cyan
