! ------------------------------------------------------------------------------
! ROFI Color theme
! ------------------------------------------------------------------------------
rofi.color-enabled: true
rofi.color-window: argb:d90a1b2a, #0a1b2a, #0a1b2a
rofi.color-normal: #0a1b2a, #ddc9dd, #0a1b2a, #3c566c, #ddc9dd
rofi.color-active: #0a1b2a, #268bd2, #0a1b2a, #268bd2, #fdf6e3
rofi.color-urgent: #0a1b2a, #dc322f, #0a1b2a, #dc322f, #fdf6e3

! special
*.foreground:   #ddc9dd
*.background:   #0a1b2a
*.cursorColor:  #ddc9dd

! black
*.color0:       #12293e
*.color8:       #3c566c

! red
*.color1:       #d4617e
*.color9:       #c47488

! green
*.color2:       #55a186
*.color10:      #a9cbbf

! yellow
*.color3:       #ccbaa4
*.color11:      #ccc1b3

! blue
*.color4:       #5e839d
*.color12:      #6a889d

! grey
*.color5:       #5f5f75
*.color13:      #868697

! lightgreen
*.color6:       #79b49f
*.color14:      #add9ca

! white
*.color7:       #e0d8e9
*.color15:      #e4dfe9
